syn match sqlNumber		
    \ ,\v(^|[^._[:alnum:]])\zs\-=(\d+\.=\d*|\.\d+)(e[+-]=\d+)=\ze($|[^._[:alnum:]]),

" Comments :
syn region sqlCComment    start="/\*"  end="\*/" contains=sqlTodo,sqlCComment
syn match sqlComment	"--.*$" contains=sqlTodo

syn sync ccomment sqlCComment
