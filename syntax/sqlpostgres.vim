function! s:toDict(list)
    let dict = {}

    for l in a:list
        let dict[tolower(l)] = ''
    endfor

    return dict
endfunction

function! s:subtract(from, what)
    if type(a:what) == type({})
        let list = keys(a:what)
    else
        let list = a:what
    endif

    for l in list
        if has_key(a:from, l)
            unlet a:from[l] 
        endif
    endfor
endfunction

function! s:synMatch(group, string)
    exec 'syn match ' . a:group .' !\<' .
        \ substitute(a:string, '\s\+', '\\_s\\+', 'g')  . '\>!'
endfunction

function! s:synMatchDict(group, dict)
    for string in sort(keys(a:dict))
        call s:synMatch(a:group, string)
    endfor
endfunction

" Make dictionaries from lists in order to avoid diplications
let d_keywords = s:toDict(sql_keywords)
let d_functions = s:toDict(sql_functions)
let d_commands = s:toDict(sql_commands)
let d_types = s:toDict(sql_types)

" abbreviated type names
call extend(d_types, s:toDict(['bool', 'int']))
call extend(d_types,
    \ s:toDict(map(sql_types, "substitute(v:val, '\\s.*', '', '')")))

" remove the most silly keywords
call s:subtract(d_keywords, ['a', 'id'])

" remove words used in types & commands
call s:subtract(d_keywords,
    \ s:toDict(split(join(sql_commands + sql_types, ' '))))

" restore some of the removed words
call extend(d_keywords, s:toDict(['as', 'into', 'group', 'language']))

" remove functions which names equal to type name or keyword
call s:subtract(d_functions, d_types)
call s:subtract(d_functions, d_keywords)

" Define syntax matches for the dictionaries
call s:synMatchDict('sqlKeyword', d_keywords)
call s:synMatchDict('sqlFunction', d_functions)
call s:synMatchDict('sqlCommand', d_commands)
call s:synMatchDict('sqlType', d_types)
call s:synMatchDict('sqlParameter', s:toDict(sql_parameters))

